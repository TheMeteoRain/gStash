{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [
            true,
            "never"
        ],
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "indent": [
            true,
            "spaces",
            2
        ],
        "no-console": false,
        "ordered-imports": true,
        "eofline": true,
        "no-submodule-imports": false
    },
    "rulesDirectory": []
}